{
  "name": "zalog",
  "version": "3.0.0",
  "description": "ZA-LOG - Hệ thống quản lý log khách hàng với dual mode support",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run autodeploy\" \"npm run server:dev\"",
    "server:dev": "node --watch server.js",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "autodeploy": "node watcher.js",
    "build": "echo 'No build step required for static files'",
    "test": "echo 'No tests specified'",
    "lint": "echo 'No linter configured'",
    "clean": "rm -rf .vercel node_modules",
    "vercel:install": "npm install",
    "vercel:build": "npm run build",
    "vercel:dev": "vercel dev",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1"
  },
  "keywords": [
    "log",
    "customer",
    "management",
    "excel",
    "grid",
    "shipping",
    "trip",
    "vercel",
    "zalog"
  ],
  "author": {
    "name": "Hung Duong",
    "email": "hung@example.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hungduong/zalog.git"
  },
  "bugs": {
    "url": "https://github.com/hungduong/zalog/issues"
  },
  "homepage": "https://zalog.vercel.app",
  "dependencies": {
    "express": "^4.18.2",
    "chokidar": "^3.6.0",
    "concurrently": "^8.2.2"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "vercel": "^32.5.0"
  },
  "optionalDependencies": {
    "fsevents": "^2.3.3"
  },
  "files": [
    "server.js",
    "index.html",
    "style.css",
    "*.js",
    "api/",
    "public/",
    "README.md"
  ],
  "vercel": {
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "/server.js"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "config": {
    "port": 3000,
    "host": "localhost"
  },
  "private": false,
  "funding": {
    "type": "individual",
    "url": "https://github.com/sponsors/hungduong"
  }
}